{
  "name": "quality-assurance-regression",
  "version": "1.0.0",
  "description": "PetClinic - Automação",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.31.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-url": "^1.0.4",
    "concurrently": "^7.0.0",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.5.0",
    "cypress": "^10.8.0",
    "cypress-multi-reporters": "^1.6.1",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-slack-reporter": "^1.2.1",
    "cypress-sql-server": "^1.0.0",
    "cypress-xpath": "^1.6.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^6.6.6",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "mochawesome-screenshots": "^1.7.3",
    "prettier": "^2.6.0",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "^8.3.1",
    "protractor-cucumber-steps": "^1.3.11",
    "sql-cli": "^0.6.2",
    "standard": "^16.0.4"
  },
  "main": "protractor conf.js",
  "scripts": {
    "alluregenerate": "allure generate allure-results/ --clean",
    "allureopen": "allure open allure-report/",
    "allureclear": "rm -r allure-results allure-report",
    "cy:clean": "rm -r allure-results allure-report",
    "executaTesteRegressao": "cypress run --headless",
    "executaTeste": "npx cypress run --spec 'cypress/e2e/GUI/SignUp/**/*'",
    "merge": "mochawesome-merge cypress/reports/mochawesome-report/*.json > cypress/reports/output.json",
    "generate_mochawesome_report": "marge cypress/reports/output.json --reportDir ./ --inline",
    "delete_reports": "node deleteReports.js && rm -rf Rel_Compl.json && rm -rf cypress/screenshots",
    "clean:reports": "(if exist cypress\\reports (rmdir /Q /S cypress\\reports)) && mkdir cypress\\reports && mkdir cypress\\reports\\mocha  &&  mkdir cypress\\reports\\mochareports",
    "cypress:open": "cypress open",
    "open": "npx cypress open",
    "junta-json": "npx mochawesome-merge 'cypress/reports/*.json' > Rel_Compl.json",
    "gera-rel-html": "npx marge Rel_Compl.json -f Relatório-Testes -o cypress/reports/",
    "build:dev": "npm run test-headless-dev && npm run junta-gera-rel",
    "build:hml": "npm run test-headless-hml && npm run junta-gera-rel",
    "junta-gera-rel": "npm run junta-json && npm run gera-rel-html",
    "apaga_dados_bd": "npx cypress run --headless -b chrome --spec 'cypress/integration/BD/deleta_dados_bd.js'",
    "test-headless": "cypress run --headless",
    "test-headless-dev": "cypress run --headless --env configFile=dev",
    "test-headless-hml": "cypress run --headless --env configFile=hml",
    "clean-reports": "rm -rf cypress/reports",
    "clean-screenshots": "rm -rf cypress/screenshots",
    "clean-videos": "rm -rf cypress/videos",
    "clean-all": "npm run clean-reports && npm run clean-screenshots && npm run clean-videos"   
  },
  "standard": {
    "globals": []
  },
  "author": "QA",
  "license": "MIT",
  "dependencies": {
    "-": "0.0.1",
    "blob": "^0.1.0",
    "chromedriver": "^93.0",
    "faker-br": "^0.4.1",
    "g": "^2.0.1",
    "mariadb": "^2.5.4",
    "mocha-allure-reporter": "^1.4.0",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.5.1",
    "mssql": "^7.2.1",
    "mysql2": "^2.3.0",
    "npm": "^7.23.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2",
    "tedious": "^12.2.0",
    "tedious-connection-pool": "^1.0.5",
    "yarn": "^1.22.11"
  }
}
